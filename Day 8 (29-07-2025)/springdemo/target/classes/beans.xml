<beans xmlns="http://www.springframework.org/schema/beans"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.springframework.org/schema/beans
                               http://www.springframework.org/schema/beans/spring-beans.xsd">
                         
                               
  <bean id="department" class="com.springdemo.beans.Department">
  <property name="deptCode" value="JFS"/>
  <property name="deptName" value="Java Fullstack Angular"/>
  </bean>
  <bean id="clg" class="com.springdemo.beans.College">
  <constructor-arg value="Jayantadas" />
  <constructor-arg ref="department" />
  </bean>
</beans>